# Порт сервера
PORT=3000

# Режим работы
NODE_ENV=development

# Настройки кэширования
CACHE_ENABLED=true
CACHE_TTL=3600

# Настройки rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Настройки запросов
REQUEST_TIMEOUT=10000
REQUEST_RETRIES=3
REQUEST_DELAY=1000

# Логирование
LOG_LEVEL=info 